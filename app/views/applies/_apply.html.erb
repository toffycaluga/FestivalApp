
<div id="<%= dom_id apply %>">
<div class="card">
    
    <div class="fluid justify-content-end d-flex align-items-center">
     
      
     
     <% if current_user.id == @apply.user.id %>
      
     <%= render 'applies/buttons_apply', apply: @apply %>
     <% end %>
      
    </div>

    <%= render 'applies/data_apply', apply: apply %>
  <div class="card-body">
    
  </div>
  <iframe src="https://www.youtube.com/embed/<%= @video_id%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen class="embed-responsive-item" height="360px"></iframe>
  
  <% if current_user.role == "Organizador" || current_user.role == "Admin" %>
    <div class="card-footer">
      <a href="#" class="btn btn-primary">Calificar</a>
    </div>
  <% end %>
</div>


</div>


